<mapper namespace="com.gooddog.mapper.AdminMapper">


	<!-- 회원목록 불러오기 -->
		<select id="getUserInfo" resultType="UserVO" parameterType="UserVO">
				<![CDATA[
			SELECT * FROM user WHERE user_id = 0 
			]]>
